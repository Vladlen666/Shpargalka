#include <stdio.h>

int main()
{
	int a,A;
	a = 66;
	char b,B;
	b = 1;
	unsigned char c,C;
	c = 2;
	short d, D;
	d = 3;
	unsigned short f, F;
	f = 4;
	unsigned int g, G;
	g = 5;
	long j, J;
	j = 6;
	unsigned long k, K;
	k = 7;
	float l, L;
	l = 3;
	double p, P;
	p = 2.6;
	long double u, U;
	u = 4.9;

	printf(" a=%d \n b=%d \n c=%d \n d=%d \n f=%d \n g=%d \n j=%d \n k=%d \n l=%f \n p=%f \n u=%f ", a,b,c,d,f,g,j,k,l,p,u,A,B,C,D,F,G,J,K,L,P,U);

	return 0;
}
